{
    "name": "relay-fragment-type-generator",
    "displayName": "RelayFragmentTypeGenerator",
    "description": "Generates TypeScript interfaces for Relay fragments in the current source file.",
    "version": "0.0.1",
    "publisher": "alloy020",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.generateRelayFragmentInterface"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.generateRelayFragmentInterface",
                "title": "Generate Relay Fragment Interface"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "typescript": "^2.1.5",
        "vscode": "^1.0.0"
    }
}
